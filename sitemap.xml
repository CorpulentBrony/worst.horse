<?xml version="1.0" encoding="UTF-8"?>
<!--# config timefmt="%Y-%m-%d" -->
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9" xmlns:image="http://www.google.com/schemas/sitemap-image/1.1">
	<url>
		<changefreq>always</changefreq>
		<image:image>
			<image:caption><!--# echo var="siteDescription" --></image:caption>
			<image:loc>https://<!--# echo var="host" -->/image</image:loc>
			<image:title><!--# echo var="siteTitle" --></image:title>
		</image:image>
		<lastmod><!--# echo var='date_local' --></lastmod>
		<loc>https://<!--# echo var="host" --></loc> 
		<priority>1.0</priority>
	</url>
</urlset>